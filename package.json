{
  "name": "anynote",
  "version": "0.1.0",
  "description": "a private note solution",
  "main": "index.ts",
  "scripts": {
    "run": "electron --disable-gpu src/desktop/main.js",
    "start": "webpack-dev-server --mode development --port 20719 --hot",
    "build": "webpack --mode production",
    "eslint": "eslint src/view --ext .tsx,.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zsimline/anynote.git"
  },
  "keywords": [
    "note"
  ],
  "author": "Mxsyx <mxsyxin@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zsimline/anynote/issues"
  },
  "homepage": "https://github.com/zsimline/anynote#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**": [
      "prettier --config .editorconfig --write",
      "eslint src --ext .tsx,.ts"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@tinymce/tinymce-react": "^3.6.0",
    "@types/node-uuid": "0.0.28",
    "electron": "^9.1.0",
    "node-uuid": "^1.4.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.0",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "typescript": "^3.9.6",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.4",
    "eslint": "^7.3.1",
    "eslint-plugin-react": "^7.20.3",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "prettier": "^2.0.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "html-webpack-plugin": "^4.3.0",
    "babel-loader": "^8.1.0",
    "style-loader": "^1.2.1",
    "css-loader": "^3.6.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "@types/react-dom": "^16.9.8"
  }
}
